  "main": "server.js",
  "type": "module",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node server.js",
    "dev:client": "vite --port 5176 --strictPort",
